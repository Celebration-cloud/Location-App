{
  "expo": {
    "name": "My Location App",
    "slug": "my-location-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/location_Icon.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "android"
    ],
    "newArchEnabled": true,
    "android": {
      
      "package": "com.celebration.locationapp",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/location_Icon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0a7ea4"
      },
      
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "*.yourcompany.com",
            "pathPrefix": "/"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/location_Icon.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a7ea4"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bc82867e-0481-4ff1-92b2-d7987e4727d7"
      }
    },
    "owner": "celebration"
    "bu"
  }
}
